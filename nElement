function nElement (arr, n) {

    let result = [];

    for (let i = 0; i<arr.length; i+= n) {
        
        result.push (arr [i]);
    }
    
    return result;
}
// nElement (['5', '20', '31', '4', '20'], 2);
console.log (nElement (['dsa', 'asd', 'test', 'tset'], 2));
// nElement (['1', '2', '3', '4', '5'], 6);
