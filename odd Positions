function oddPositions (arr) {

    let result = [];
   
    for (let i = 0; i < arr.length; i++) {
        if (i % 2 !== 0) {
            let doubledNum = arr [i] * 2;
            result.push (doubledNum);
        }  
    }
    result.reverse ();
    console.log (result.join (' '));
}
oddPositions ([10, 15, 20, 25]);
// oddPositions ([3, 0, 10, 4, 7, 3]);
