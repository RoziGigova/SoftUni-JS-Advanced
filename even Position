function evenPosition (arr) {

    let evenNums = '';

    for (let num=0; num< arr.length; num++) {
        if (num % 2 === 0) {
            evenNums += arr [num] + ' ';
        }
    }
console.log (evenNums.trim ());

    }


evenPosition (['20', '30', '40','50', '60']);
evenPosition (['5', '10']);
